{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": true,
    // "paths": {
    //   "@/*": ["./src/*"]
    // },
    "baseUrl": "."
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/Fuyou.tsx", "src/components/Shindan.jsx", "src/pages/Taisyoku.tsx", "src/components/Header.tsx", "src/components/ToggleButton.tsx", "src/components/Navigation.tsx"],
  "exclude": ["node_modules"]
}
